{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"declaration": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitThis": true,
		"noUnusedLocals": true
	}
}
